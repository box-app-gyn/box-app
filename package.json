{
  "name": "cerrado-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:mobile": "next dev -H 0.0.0.0 -p 3000",
    "dev:mobile:config": "NODE_ENV=development next dev -H 0.0.0.0 -p 3000 -c next.config.mobile.js",
    "dev:mobile:https": "node scripts/dev-mobile-https.js",
    "ip": "node scripts/get-local-ip.js",
    "test:pwa": "node scripts/test-pwa.js",
    "check:pwa": "node scripts/check-pwa.js",
    "deploy:pwa": "node scripts/deploy-pwa.js",
    "build": "next build",
    "build:app-hosting": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "node scripts/cleanup.js",
    "clean:all": "node scripts/cleanup.js && rm -rf node_modules package-lock.json && npm install",
    "clean:cache": "npm cache clean --force && npx next clean",
    "setup:vertex-ai": "./scripts/setup-vertex-ai.sh",
    "setup:vertex-ai:fallback": "./scripts/setup-vertex-ai-fallback.sh",
    "setup:vertex-ai:check": "gcloud projects describe interbox-app-8d400 --format='value(projectId)'",
    "vertex-ai:logs": "gcloud logging read 'resource.type=aiplatform.googleapis.com/Model' --limit=20",
    "vertex-ai:status": "gcloud services list --enabled --filter='name:aiplatform.googleapis.com'",
    "security:check": "npm audit && npm audit fix",
    "security:fix": "npm audit fix --force",
    "deps:update": "npm update && npm audit fix",
    "deps:check": "npm outdated",
    "test": "echo 'No tests configured yet'",
    "test:watch": "echo 'No tests configured yet'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup-github-deploy": "node scripts/setup-github-deploy.js",
    "deploy": "npm run build && firebase deploy --only apphosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:all": "firebase deploy"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^0.2.0",
    "apexcharts": "^4.7.0",
    "canvas-confetti": "^1.9.3",
    "critters": "^0.0.23",
    "dotenv": "^17.0.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.2",
    "framer-motion": "^12.19.2",
    "node-fetch": "^3.3.2",
    "react": "^18.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-scroll-parallax": "^3.4.5"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.30.1",
    "eslint-config-next": "^15.3.4",
    "eslint-plugin-react": "^7.37.5",
    "firebase-tools": "^14.9.0",
    "next": "^15.3.4",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
