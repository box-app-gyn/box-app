{
  "name": "cerrado-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:firebase": "cp next.config.firebase.js next.config.js && next build && cp next.config.js next.config.firebase.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "setup:vertex-ai": "./scripts/setup-vertex-ai.sh",
    "setup:vertex-ai:fallback": "./scripts/setup-vertex-ai-fallback.sh",
    "setup:vertex-ai:check": "gcloud projects describe interbox-app-8d400 --format='value(projectId)'",
    "vertex-ai:logs": "gcloud logging read 'resource.type=aiplatform.googleapis.com/Model' --limit=20",
    "vertex-ai:status": "gcloud services list --enabled --filter='name:aiplatform.googleapis.com'"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^0.2.0",
    "apexcharts": "^4.7.0",
    "dotenv": "^17.0.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.2",
    "framer-motion": "^12.19.2",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.0.0",
    "react-scroll-parallax": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
