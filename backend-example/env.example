# Configurações do Servidor
PORT=3001
NODE_ENV=production

# Frontend URL
FRONTEND_URL=https://cerradointerbox.com.br

# Google Cloud / Vertex AI
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=./vertex-ai-sa-key.json

# Banco de Dados
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/cerrado-ai?retryWrites=true&w=majority

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_SAMPLE_RATE=0.1

# CORS
CORS_ORIGIN=https://cerradointerbox.com.br

# Socket.IO
SOCKET_CORS_ORIGIN=https://cerradointerbox.com.br 